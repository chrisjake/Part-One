{
    "plugins": [
        "mathjax-beta",
        "wide",
        "ungrey",
        "expandable-chapters-small",
        "git-author"
    ],
    "pluginsConfig": {
          "git-author": {
              "position": "bottom",
              "modifyTpl": "Last updated on {timeStamp}",
              "createTpl": false,
              "timeStampFormat": "YYYY-MM-DD"
          }
    }
}